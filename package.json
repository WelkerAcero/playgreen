{
    "name": "playgreen",
    "version": "1.0.0",
    "description": "Instalaci√≥n de dependencias",
    "main": "./public/MyServer.ts",
    "scripts": {
        "devdocker": "ts-node public/MyServer.ts",
        "dev": "npx nodemon ./public/MyServer.ts",
        "start": "node ./dist/public/MyServer.js",
        "ts-config": "npx tsc --init",
        "build": "tsc -p tsconfig.json",
        "test": "echo \"Error: no test specified\" && exit 1",
        "model": "node ./config/modelConfig/app.js --model=$npm_config_model",
        "controller": "node ./config/controllerConfig/app.js --controller=$npm_config_controller",
        "dbgenerate": "npx prisma generate --schema=./Database/schema.prisma",
        "migrate": "npx prisma migrate dev --name init --schema=./Database/schema.prisma",
        "db_deploy": "npx prisma migrate deploy",
        "db_push": "npx prisma db push --preview-feature --schema=./Database/schema.prisma",
        "import_sql": "psql -h db -U postgres -d postgres -f ./Database/medical_db.sql"
    },
