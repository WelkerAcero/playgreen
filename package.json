        "@types/circular-json": "^0.4.0",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.13",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/response-time": "^2.3.5",
        "cross-env": "^7.0.3",
        "nodemon": "^3.0.1",
        "prettier": "^2.8.8",
        "prisma": "^4.16.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "@prisma/client": "^4.16.2",
        "@types/express": "^4.17.21",
        "@types/node": "^18.16.18",
        "@types/nodemailer": "^6.4.13",
        "@vonage/server-sdk": "^3.1.2",
        "circular-json": "^0.5.9",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "decimal.js": "^10.4.3",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.1",
        "mysql": "^2.18.1",
        "nodemailer": "^6.9.7",
        "response-time": "^2.3.2",
        "yargs": "^17.7.2"
    }
